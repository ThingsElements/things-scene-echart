{
  "name": "@things-elements/things-scene-echart",
  "flat": true,
  "version": "0.1.0",
  "description": "An echart component for things-scene.",
  "main": "things-scene-echar-min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-echart.js src/index.js & polymer serve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-echart.js && uglifyjs things-scene-echart.js -o things-scene-echart-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-echart.git"
  },
  "author": "",
  "license": "http://things-scene.hatiolab.com/EULA/",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-echart/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-echart#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglify-js": "^3.0.17",
    "watchify": "^3.3.1",
    "node-canvas": "Automattic/node-canvas",
    "gulp": "^3.8.5",
    "gulp-header-license": "^1.0.9"
  },
  "dependencies": {
    "@hatiolab/things-scene":"^2.2.36"
  }
}
